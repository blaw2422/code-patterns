             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Patterns.Tools.DocGenerator"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <ResourceDictionary>
				<ResourceDictionary.MergedDictionaries>
					<ResourceDictionary>
						<local:DocGeneratorBootstrapper x:Key="bootstrapper" />
					</ResourceDictionary>
				</ResourceDictionary.MergedDictionaries>
			</ResourceDictionary>
    </Application.Resources>
</Application>
